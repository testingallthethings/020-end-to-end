{
  "name": "braddle/end-to-end-testing",
  "description": "Demonstrating End to End Testing and integration testing of a simple REST API",
  "minimum-stability": "dev",
  "license": "MIT",
  "authors": [
    {
      "name": "Mark Bradley",
      "email": "braddle@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Braddle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Braddle\\": [
        "test/e2e",
        "test/unit",
        "test/integration"
      ]
    }
  },
  "require": {
    "slim/slim": "4.4.0",
    "slim/psr7": "1.0.0",
    "doctrine/orm": "2.7.2",
    "symfony/yaml": "2.*"
  },
  "require-dev": {
    "phpunit/phpunit": "9.0.2",
    "guzzlehttp/guzzle": "7.0.0-beta.1"
  }
}
